{
  "name": "token-factory",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run compile && next build",
    "compile": "TS_NODE_PROJECT='./tsconfig.hardhat.json' npx hardhat compile",
    "start": "next start",
    "clean": "rm -rf .next out cache artifacts typechain-types",
