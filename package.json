    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@openzeppelin/contracts-upgradeable": "^5.2.0",
    "@openzeppelin/hardhat-upgrades": "^3.9.0",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "chai": "^4.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "hardhat": "^2.22.18",
    "postcss": "^8",
    "tailwindcss": "^3",
    "typescript": "^5"
