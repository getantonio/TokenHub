# Local Development Only - Copy to .env.local
# For Firebase deployment, use: firebase functions:config:set contracts.sepolia_factory_address="0x..."

# Deployment
PRIVATE_KEY=your_private_key_here

# RPC URLs
SEPOLIA_RPC_URL=https://rpc.sepolia.org
POLYGON_AMOY_RPC_URL=https://polygon-amoy.infura.io/v3/YOUR_INFURA_KEY
OP_SEPOLIA_RPC_URL=https://sepolia.optimism.io
ARBITRUM_SEPOLIA_RPC_URL=https://sepolia-rollup.arbitrum.io/rpc

# Block Explorer API Keys
ETHERSCAN_API_KEY=your_etherscan_api_key
POLYGONSCAN_API_KEY=your_polygonscan_api_key
OPTIMISM_API_KEY=your_optimism_api_key
ARBISCAN_API_KEY=your_arbiscan_api_key

# Factory Addresses - V1
NEXT_PUBLIC_SEPOLIA_FACTORY_ADDRESS_V1=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_POLYGON_AMOY_FACTORY_ADDRESS_V1=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_OP_SEPOLIA_FACTORY_ADDRESS_V1=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_ARBITRUM_SEPOLIA_FACTORY_ADDRESS_V1=0x0000000000000000000000000000000000000000

# Factory Addresses - V2
NEXT_PUBLIC_SEPOLIA_FACTORY_ADDRESS_V2=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_POLYGON_AMOY_FACTORY_ADDRESS_V2=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_OP_SEPOLIA_FACTORY_ADDRESS_V2=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_ARBITRUM_SEPOLIA_FACTORY_ADDRESS_V2=0x0000000000000000000000000000000000000000

# Public RPC URLs (for frontend)
NEXT_PUBLIC_SEPOLIA_RPC_URL=https://rpc.sepolia.org
NEXT_PUBLIC_POLYGON_AMOY_RPC_URL=https://rpc.amoy.testnet.polygon.com
NEXT_PUBLIC_OP_SEPOLIA_RPC_URL=https://sepolia.optimism.io
NEXT_PUBLIC_ARBITRUM_SEPOLIA_RPC_URL=https://sepolia-rollup.arbitrum.io/rpc

# Mainnet (if needed)
# NEXT_PUBLIC_MAINNET_FACTORY_ADDRESS=0x0000000000000000000000000000000000000000
# NEXT_PUBLIC_MAINNET_RPC_URL=https://eth.llamarpc.com
